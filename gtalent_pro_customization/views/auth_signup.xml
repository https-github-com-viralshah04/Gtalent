    <template id="signup_fields" inherit_id="auth_signup.fields" name="Auth Signup/ResetPassword form fields">
        <xpath expr="//div[hasclass('field-login')]" position="replace">
            <div class="form-group field-login">
                <label for="login">Your Email</label><span style="color:#FF0000;">*</span>
                <input type="text" name="login" t-att-value="login" id="login" class="form-control form-control-sm" autofocus="autofocus"
                    autocapitalize="off" required="required" t-att-readonly="'readonly' if only_passwords else None"/>
            </div>
        </xpath>
        <xpath expr="//div[hasclass('field-name')]" position="replace">
            <div class="form-group field-name">
                <label for="name">Your Name</label><span style="color:#FF0000;">*</span>
                <input type="text" name="name" t-att-value="name" id="name" class="form-control form-control-sm" placeholder="e.g. John Doe"
                    required="required" t-att-readonly="'readonly' if only_passwords else None"
                    t-att-autofocus="'autofocus' if login and not only_passwords else None" />
            </div>
            <div class="form-group field-name">
                <label for="name">Mobile Number</label><span style="color:#FF0000;">*</span>
                <input type="text" name="phone" id="mobile" class="form-control form-control-sm"
                    required="required"/>
            </div>
        </xpath>
        <xpath expr="//div[hasclass('field-password')]" position="replace">
            <div class="form-group field-password pt-2">
                <label for="password">Password</label><span style="color:#FF0000;">*</span>
                <input type="password" name="password" id="password" class="form-control form-control-sm"
                    required="required" t-att-autofocus="'autofocus' if only_passwords else None"/>
            </div>

            <div class="alert alert-warning" id="medium_password">Your password strength is medium</div>
            <div class="alert alert-danger" id="weak_password">Your password strength is weak</div>
            <div class="alert alert-success" id="strong_password">Your password strength is strong</div>
        </xpath>
        <xpath expr="//div[hasclass('field-confirm_password')]" position="replace">
            <div class="form-group field-confirm_password">
                <label for="confirm_password">Confirm Password</label><span style="color:#FF0000;">*</span>
                <input type="password" name="confirm_password" id="confirm_password" class="form-control form-control-sm" required="required"/>
            </div>

            <t t-if="usertype == 'candidate'">
            <div id="extra_details">
                <div class="form-group field-name">
                    <label for="date_of_birth">Date of Birth</label><span style="color:#FF0000;">*</span>
                    <input type="date" name="date_of_birth" id="date_of_birth" class="form-control form-control-sm" placeholder="dd/mm/yy"
                        required="required"/>
                </div>
            </div>
            </t>
        </xpath>
        <!--<xpath expr="//div[hasclass('field-name')]" position="after">-->
